/* pages/rename/rename.wxss */
.container{
  width: 100%;
  height: 100%;
}
.cBody{
  width: 92%;
  height: 100%;
  padding: 20rpx 4%;
}
.title{
  font-size: 32rpx;
  line-height: 60rpx;
  color: #333333;
}
.input{
  height: 58rpx;
  margin-top: 10rpx;
  padding: 20rpx 4%;
  width: 92%;
  line-height: 58rpx;
  color: #333333;
  font-size: 32rpx;
  border: 1rpx solid #d1d1d1;
  border-radius: 10rpx;
}
.phcolor{
  color: #999999;
}
.changeBtn{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  padding: 0;
  background: #6ea8f7;
  color: #ffffff;
  font-size: 34rpx;
  border-radius: 50rpx;
  margin-top: 100rpx;
}